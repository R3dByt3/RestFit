<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:FitnessTrackingAndPlanning.Views"
                    xmlns:viewModels="clr-namespace:FitnessTrackingAndPlanning.ViewModels">

    <DataTemplate DataType="{x:Type viewModels:LoginViewModel}">
        <views:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:NavigationViewModel}">
        <views:NavigationView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:TrainingViewModel}">
        <views:TrainingView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:FriendsViewModel}">
        <views:FriendsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:HealthDataViewModel}">
        <views:HealthDataView/>
    </DataTemplate>

    <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>

</ResourceDictionary>