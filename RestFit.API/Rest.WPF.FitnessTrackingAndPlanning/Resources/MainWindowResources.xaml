<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:FitnessTrackingAndPlanning.Views"
                    xmlns:viewModels1="clr-namespace:Rest.WPF.FitnessTrackingAndPlanning.ViewModels">

    <DataTemplate DataType="{x:Type viewModels1:LoginViewModel}">
        <views:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels1:NavigationViewModel}">
        <views:NavigationView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels1:TrainingViewModel}">
        <views:TrainingView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels1:FriendsViewModel}">
        <views:FriendsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels1:HealthDataViewModel}">
        <views:HealthDataView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels1:HistoryViewModel}">
        <views:HistoryView/>
    </DataTemplate>

</ResourceDictionary>