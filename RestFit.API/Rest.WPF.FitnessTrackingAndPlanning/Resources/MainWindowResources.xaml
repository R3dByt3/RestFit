<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewModels="clr-namespace:Rest.WPF.FitnessTrackingAndPlanning.ViewModels"
                    xmlns:views="clr-namespace:Rest.WPF.FitnessTrackingAndPlanning.Views">

    <DataTemplate DataType="{x:Type viewModels:LoginViewModel}">
        <views:LoginView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:NavigationViewModel}">
        <views:NavigationView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:TrainingViewModel}">
        <views:TrainingView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:FriendsViewModel}">
        <views:FriendsView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:HealthDataViewModel}">
        <views:HealthDataView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewModels:HistoryViewModel}">
        <views:HistoryView/>
    </DataTemplate>

</ResourceDictionary>